{
  "name": "@alextheman/components",
  "version": "4.0.0",
  "description": "A package containing common React components used across my projects",
  "license": "ISC",
  "author": "alextheman",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "change-major": "npm version major -m \"Change version number to v%s\"",
    "change-minor": "npm version minor -m \"Change version number to v%s\"",
    "change-patch": "npm version patch -m \"Change version number to v%s\"",
    "create-local-package": "npm run build && rm -f alextheman-components-*.tgz && npm pack",
    "format": "prettier --write --parser typescript \"src/**/*.{ts,tsx}\" && eslint --fix --suppress-all \"src/**/*.{ts,tsx}\" \"package.json\" && rm -f eslint-suppressions.json",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx}\" \"package.json\" && prettier --check --parser typescript \"src/**/*.{ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install' --"
  },
  "dependencies": {
    "@alextheman/utility": "^1.16.0",
    "common-tags": "^1.8.2",
    "react-live": "^4.1.8"
  },
  "devDependencies": {
    "@alextheman/eslint-plugin": "^1.14.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/common-tags": "^1.8.4",
    "@types/node": "^24.3.2",
    "@types/react": "^19.1.13",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "styled-components": "^6.1.19",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "react": "^19.1.0",
    "react-router-dom": "^7.6.2"
  }
}
