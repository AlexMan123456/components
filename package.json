{
  "name": "@alextheman/components",
  "version": "4.3.1",
  "description": "A package containing common React components used across my projects",
  "license": "ISC",
  "author": "alextheman",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "change-major": "npm version major -m \"Change version number to v%s\"",
    "change-minor": "npm version minor -m \"Change version number to v%s\"",
    "change-patch": "npm version patch -m \"Change version number to v%s\"",
    "create-local-package": "npm run build && rm -f alextheman-components-*.tgz && npm pack",
    "format": "npm run format-prettier && npm run format-eslint",
    "format-eslint": "eslint --fix --suppress-all \"src/**/*.{ts,tsx}\" \"package.json\" && rm -f eslint-suppressions.json",
    "format-prettier": "npm run format-prettier-typescript && npm run format-prettier-javascript",
    "format-prettier-javascript": "prettier --write \"./**/*.js\"",
    "format-prettier-typescript": "prettier --write --parser typescript \"./**/*.{ts,tsx}\"",
    "lint": "npm run lint-tsc && npm run lint-eslint && npm run lint-prettier",
    "lint-eslint": "eslint \"src/**/*.{ts,tsx}\" \"package.json\"",
    "lint-prettier": "npm run lint-prettier-typescript && npm run lint-prettier-javascript",
    "lint-prettier-javascript": "prettier --check \"./**/*.js\"",
    "lint-prettier-typescript": "prettier --check --parser typescript \"./**/*.{ts,tsx}\"",
    "lint-tsc": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install' --"
  },
  "dependencies": {
    "@alextheman/utility": "^1.22.0",
    "common-tags": "^1.8.2",
    "react-live": "^4.1.8"
  },
  "devDependencies": {
    "@alextheman/eslint-plugin": "^1.14.2",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@types/common-tags": "^1.8.4",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "styled-components": "^6.1.19",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "react": "^19.1.0",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.6.2"
  }
}
